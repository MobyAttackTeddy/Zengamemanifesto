@model zengamemanifesto.Models.VideoSet

@{
    ViewBag.Title = "Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>@Html.DisplayFor(model => model.Heading)</h2>

<div>

    <dl class="dl-horizontal">

        <dd>
            <iframe width="560" height="315" src="@Html.DisplayTextFor(model => model.EmbededUrl)" frameborder="0" allowfullscreen></iframe>
            
        </dd>
        <dd>
            @Html.DisplayFor(model => model.Text)
        </dd>

    </dl>
</div>
<p>
    @Html.ActionLink("Back to Videos", "Index")
</p>
